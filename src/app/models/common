export class Client {
  id!: string;
  code!: string;
  name!: string;
  description!: string;
  image!: string;
  price!: number;
  category!: string;
  quantity!: number;
  inventoryStatus!: string;
  rating!: number;
  orders!: Order[];
}

export class Order {
  id!: string;
  clientCode!: string;
  date!: string;
  amount!: number;
  quantity!: number;
  customer!: string;
  status!: string;
}